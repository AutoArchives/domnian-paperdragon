--- /dev/null
+++ b/src/main/java/com/domnian/paperdragon/event/ClientBrandReceivedEvent.java
@@ -0,0 +1,35 @@
+package com.domnian.paperdragon.event;
+
+import org.bukkit.entity.Player;
+import org.bukkit.event.Event;
+import org.bukkit.event.HandlerList;
+
+public class ClientBrandReceivedEvent extends Event {
+
+    private static final HandlerList handlers = new HandlerList();
+
+    Player player;
+    String brand;
+
+    public ClientBrandReceivedEvent(Player who, String brand) {
+        this.player = who;
+        this.brand = brand;
+    }
+
+    public HandlerList getHandlers() {
+        return handlers;
+    }
+
+    public static HandlerList getHandlerList() {
+        return handlers;
+    }
+
+    public Player getPlayer() {
+        return player;
+    }
+
+    public String getBrand() {
+        return brand;
+    }
+
+}
