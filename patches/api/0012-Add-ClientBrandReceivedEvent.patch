+package com.domnian.paperdragon.event;
+
+import org.bukkit.entity.Player;
+import org.bukkit.event.Event;
+import org.bukkit.event.HandlerList;
+
+public class ClientBrandReceivedEvent extends Event {
+
+    private static final HandlerList handlers = new HandlerList();
+
+    Player player;
+    String brand;
+
+    public ClientBrandReceivedEvent(Player who, String brand) {
+        this.player = who;
+        this.brand = brand;
+    }
+
+    @Override
+    public HandlerList getHandlers() {
+        return null;
+    }
+
