index 89732a41752eda3c2ccc07e9580ded8266f8d4f0..f0271fd4a58642ab2065f8136efdaae1b9fbc015 100644
--- a/src/main/java/net/minecraft/world/level/levelgen/feature/VegetationPatchFeature.java
