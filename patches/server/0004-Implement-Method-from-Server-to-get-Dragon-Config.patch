@@ -1829,6 +1829,9 @@ public final class CraftServer implements Server {
         }
 
         @Override
+        public YamlConfiguration getDragonConfig() { return com.domnian.paperdragon.DragonConfig.config; }
+
+        @Override
         public void restart() {
             org.spigotmc.RestartCommand.restart();
         }
