From: willies952002 <admin@domnian.com>
Date: Sun, 3 Jul 2016 19:16:19 -0400
Subject: [PATCH] Implement Proxy Mode API


diff --git a/src/main/java/org/bukkit/craftbukkit/CraftServer.java b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
index 3695c5b79694de78a63d94ce05d8c79a729e06db..fe331e1f0c6da8fc6e7719bf2b6c0727972c44e5 100644
--- a/src/main/java/org/bukkit/craftbukkit/CraftServer.java
+++ b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
@@ -1,5 +1,6 @@
 package org.bukkit.craftbukkit;
 
+import com.domnian.paperdragon.ProxyMode;
 import com.domnian.paperdragon.api.EntityTasksHandler;
 import com.domnian.paperdragon.api.meta.MetaApiAccessor;
 import com.google.common.base.Charsets;
@@ -2738,4 +2739,21 @@ public final class CraftServer implements Server {
     }
 
     // Paper end
+
+    // PaperDragon start
+    public com.domnian.paperdragon.ProxyMode proxyMode = ProxyMode.UNKNOWN;
+    @Override
+    public com.domnian.paperdragon.ProxyMode getProxyMode() {
+        if (proxyMode != ProxyMode.UNKNOWN) return proxyMode;
+        if (org.spigotmc.SpigotConfig.bungee) {
+            return proxyMode = ProxyMode.BUNGEECORD;
+        } else if (com.destroystokyo.paper.PaperConfig.velocitySupport) {
+            if (com.destroystokyo.paper.PaperConfig.velocityOnlineMode) {
+                return proxyMode = ProxyMode.VELOCITY_ONLINE;
+            }
+            return proxyMode = ProxyMode.VELOCITY_OFFLINE;
+        }
+        return proxyMode = ProxyMode.NONE;
+    }
+    // PaperDragon end
 }
