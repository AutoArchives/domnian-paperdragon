Subject: [PATCH] Implement ArrowHitBlockEvent

---
 src/main/java/net/minecraft/server/EntityArrow.java | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/src/main/java/net/minecraft/server/EntityArrow.java b/src/main/java/net/minecraft/server/EntityArrow.java
index 37cb17c..c100bb9 100644
--- a/src/main/java/net/minecraft/server/EntityArrow.java
+++ b/src/main/java/net/minecraft/server/EntityArrow.java
@@ -359,7 +359,9 @@ public abstract class EntityArrow extends Entity implements IProjectile {
             this.shake = 7;
             this.setCritical(false);
             if (iblockdata.getMaterial() != Material.AIR) {
+                if (new com.domnian.paperdragon.event.ArrowHitBlockEvent(this.getBukkitEntity(), world.getWorld().getBlockAt(blockposition.getX(), blockposition.getY(), blockposition.getZ())).callEvent()) { // PaperDragon - Import Patch From EMC
                 this.aw.a(this.world, blockposition, iblockdata, (Entity) this);
+                } // PaperDragon - Import Patch From EMC
             }
         }
 
