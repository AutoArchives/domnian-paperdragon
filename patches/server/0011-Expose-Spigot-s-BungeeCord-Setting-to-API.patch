From 61201a924bb463d09cd6b64ca1f08aa5a1a6f5ab Mon Sep 17 00:00:00 2001
From: willies952002 <admin@domnian.com>
Date: Sun, 3 Jul 2016 19:16:19 -0400
Subject: [PATCH] Expose Spigot's BungeeCord Setting to API

---
 src/main/java/org/bukkit/craftbukkit/CraftServer.java | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/src/main/java/org/bukkit/craftbukkit/CraftServer.java b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
index 064fdb62..d307b84c 100644
--- a/src/main/java/org/bukkit/craftbukkit/CraftServer.java
+++ b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
@@ -1974,6 +1974,14 @@ public final class CraftServer implements Server {
         return com.destroystokyo.paper.PaperConfig.suggestPlayersWhenNullTabCompletions;
     }
 
+    // PaperDragon start - Expose BungeeCord Setting
+    @Override
+    public boolean isBungeeEnabled() {
+        return org.spigotmc.SpigotConfig.bungee;
+    }
+    // PaperDragon end
+
+    // Paper start
     public com.destroystokyo.paper.profile.PlayerProfile createProfile(@Nonnull UUID uuid) {
         return createProfile(uuid, null);
     }
